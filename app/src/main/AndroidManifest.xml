<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.smartwallet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pig"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WalletActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".IncomeActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".HintActivity" />
    </application>

</manifest>
